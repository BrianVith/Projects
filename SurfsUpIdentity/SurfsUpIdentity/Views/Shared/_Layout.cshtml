<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-BoardType" content="text/html; charset=UTF-8">
    <meta name="description" content="Bingo One page parallax responsive HTML Template ">

    <meta name="author" content="Themefisher.com">

    <title>SurfsUp</title>

    <!-- Mobile Specific Meta
      ================================================== -->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Favicon -->
    <link rel="shortcut icon" type="image/x-icon" href="~/images/favicon.png" />

    <!-- CSS
    ================================================== -->
    <!-- Themefisher Icon font -->
    <link rel="stylesheet" href="~/plugins/themefisher-font/style.css">
    <!-- bootstrap.min css -->
    <link rel="stylesheet" href="~/plugins/bootstrap/css/bootstrap.min.css">
    <!-- Lightbox.min css -->
    <link rel="stylesheet" href="~/plugins/lightbox2/dist/css/lightbox.min.css">
    <!-- animation css -->
    <link rel="stylesheet" href="~/plugins/animate/animate.css">
    <!-- Slick Carousel -->
    <link rel="stylesheet" href="~/plugins/slick/slick.css">
    <!-- Main Stylesheet -->
    <link rel="stylesheet" href="~/css/style.css">

</head>

<body id="body">

    <!--
     Start Preloader
     ==================================== -->
    <div id="preloader">
        <div class='preloader'>
            <span></span>
            <span></span>
            <span></span>
            <span></span>
            <span></span>
            <span></span>
        </div>
    </div>
    <!--
    End Preloader
    ==================================== -->
    <!--
    Fixed Navigation
    ==================================== -->
    <header class="navigation fixed-top">
        <div class="container">
            <!-- main nav -->
            <nav class="navbar navbar-expand-lg navbar-light">
                <!-- logo -->
                <a class="navbar-brand logo" href="@Url.Action("Index","Home")">
                    <img class="logo-default" src="~/images/logos/logo-bold.png" alt="Logo" width="130" height="42"/>
                    <img class="logo-white" src="~/images/logos/logo-white-bold.png" alt="Logo" width="130" height="42"/>
                   
                </a>
                <!-- /logo -->
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navigation"
                        aria-controls="navigation" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navigation">
                    <ul class="navbar-nav ml-auto text-center">
                        <li class="nav-item">
                            <a class="nav-link" href="@Url.Action("Index","Home")">Forside</a>
                        </li>
                        <li class="nav-item ">
                            <a class="nav-link" href="@Url.Action("About","Home")">Om Os</a>
                        </li>
                        <li class="nav-item ">
                            <a class="nav-link" href="@Url.Action("Index","BorrowEquipment")">Lej Udstyr</a>
                        </li>
                        <li class="nav-item ">
                            <a class="nav-link" href="@Url.Action("Index","LendEquipment")">Udlej Udstyr</a>
                        </li>
                        <li class="nav-item ">
                            <a class="nav-link" href="@Url.Action("Index","SurfSpot")">Surfspots</a>
                        </li>
                        <li class="nav-item ">
                            <a class="nav-link" href="@Url.Action("Contact","Home")">Kontakt Os</a>
                        </li>
                        <li class="nav-item">
                            <partial name="_LoginPartial"/>
                        </li>
                    </ul>
                </div>
            </nav>
            <!-- /main nav -->
        </div>
    </header>
    <!--
    End Fixed Navigation
    ==================================== -->



    @RenderBody()


    <footer id="footer" class="bg-one">
        <div class="top-footer">
            <div class="container">
                <div class="row">
                    <div class="col-sm-3 col-md-3 col-lg-3">
                        <h3>Om Os</h3>
                        <p>SurfsUp forsøger at skabe en bedre planet ved at tilbyde udlejning af surfudstyr og oprydning af strande</p>
                    </div>
                    <!-- End of .col-sm-3 -->

                    <div class="col-sm-3 col-md-3 col-lg-3">
                        <ul>
                            <li><h3>Vores Udlejning</h3></li>
                            <li><a href="@Url.ActionLink("index","BorrowEquipment")">Lej Kano udstyr</a></li>
                            <li><a href="@Url.ActionLink("index","BorrowEquipment")">Lej Kitesurfing udstyr</a></li>
                            <li><a href="@Url.ActionLink("index","BorrowEquipment")">Lej Vindsurfing udstyr</a></li>
                        </ul>
                    </div>
                    <!-- End of .col-sm-3 -->

                    <div class="col-sm-3 col-md-3 col-lg-3">
                        <ul>
                            <li><h3>Hurtige Links</h3></li>
                            <li><a href="@Url.ActionLink("index","LendEquipment")">Udlej Udstyr</a></li>
                            <li><a href="@Url.ActionLink("index","BorrowEquipment")">Lej Udstyr</a></li>
                            <li><a href="@Url.ActionLink("About","Home")">Om Os</a></li>
                            <li><a href="@Url.ActionLink("Contact","home")">Kontakt Os</a></li>
                        </ul>
                    </div>
                    <!-- End of .col-sm-3 -->

                    <div class="col-sm-3 col-md-3 col-lg-3">
                        <ul>
                            <li><h3>Forbind med os på Sociale Medier</h3></li>
                            <li><a href="#">Facebook</a></li>
                            <li><a href="#">Twitter</a></li>
                        </ul>
                    </div>
                    <!-- End of .col-sm-3 -->

                </div>
            </div> <!-- end container -->
        </div>
        <div class="footer-bottom">
            <h5>Copyright @DateTime.Now.Year All rights reserved.</h5>
            <h6>Design and Developed by <a href="">SurfsUp</a></h6>
        </div>
    </footer> <!-- end footer -->
    <!-- end Footer Area
    ========================================== -->
    <!--
    Essential Scripts
    =====================================-->
    <!-- Main jQuery -->
    <script src="~/plugins/jquery/jquery.min.js"></script>
    <!-- Google Map -->
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBu5nZKbeK-WHQ70oqOWo-_4VmwOwKP9YQ"></script>
    <script src="~/plugins/google-map/gmap.js"></script>

    <!-- Form Validation -->
    <script src="~/plugins/form-validation/jquery.form.js"></script>
    <script src="~/plugins/form-validation/jquery.validate.min.js"></script>

    <!-- Bootstrap4 -->
    <script src="~/plugins/bootstrap/js/bootstrap.min.js"></script>
    <!-- Parallax -->
    <script src="~/plugins/parallax/jquery.parallax-1.1.3.js"></script>
    <!-- lightbox -->
    <script src="~/plugins/lightbox2/dist/js/lightbox.min.js"></script>
    <!-- Owl Carousel -->
    <script src="~/plugins/slick/slick.min.js"></script>
    <!-- filter -->
    <script src="~/plugins/filterizr/jquery.filterizr.min.js"></script>
    <!-- Smooth Scroll js -->
    <script src="~/plugins/smooth-scroll/smooth-scroll.min.js"></script>

    <!-- Custom js -->
    <script src="~/js/script.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
